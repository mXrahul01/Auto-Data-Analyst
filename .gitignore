# Auto Data Analyst - Comprehensive .gitignore

# ================================
# PYTHON & ENVIRONMENTS
# ================================

__pycache__/
*.py[cod]
*$py.class
*.so
build/
dist/
*.egg-info/
.installed.cfg
*.egg

# Virtual environments
.env
.venv
env/
venv/
ENV/

# ================================
# AUTO DATA ANALYST SPECIFIC
# ================================

# Environment files (contains secrets)
.env
.env.local
.env.development
.env.staging
.env.production
.env.testing

# Data directories
uploads/
temp/
temporary/
datasets/
processed/
artifacts/
models/trained/
models/cache/

# File uploads and datasets
*.csv
*.json
*.parquet
*.xlsx
*.xls
*.h5
*.hdf5
*.feather
*.pickle
*.pkl
*.joblib

# ML Model files
*.model
*.sav
*.pb
*.onnx
*.tflite

# MLflow artifacts
mlruns/
mlartifacts/
.mlflow/

# Feast feature store
.feast/
feast_repo/

# ================================
# DATABASES
# ================================

# SQLite databases
*.db
*.sqlite
*.sqlite3

# PostgreSQL dumps
*.sql
*.dump

# ================================
# CACHING & LOGS
# ================================

# Redis dumps
dump.rdb

# Cache directories
.cache/
.pytest_cache/
.mypy_cache/

# Application logs
*.log
logs/
log/

# ================================
# CLOUD & SECURITY
# ================================

# AWS
.aws/
*.pem

# GCP
service-account*.json
google-credentials*.json

# Azure
.azure/

# API Keys and secrets
secrets/
.secrets/
*.key
*.p12
*.pfx

# ================================
# IDEs & DEVELOPMENT
# ================================

# VSCode
.vscode/

# PyCharm
.idea/

# Jupyter Notebook
.ipynb_checkpoints
*.ipynb

# OS files
.DS_Store
Thumbs.db

# ================================
# KEEP THESE FILES (exceptions)
# ================================

!**/.gitkeep
!*.example
!requirements*.txt
!docker-compose.yml
!Dockerfile
!.env.example
